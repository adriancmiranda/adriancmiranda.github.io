bundle: npm run build
transpile: npm run dev --optimize_for_size --max_old_space_size=1380 --gc_interval=100 --use_strict --harmony
test: npm test -- --optimize_for_size --max_old_space_size=460 --gc_interval=100 --use_strict --harmony
web: npm start -- --optimize_for_size --max_old_space_size=920 --gc_interval=100 --use_strict --harmony
