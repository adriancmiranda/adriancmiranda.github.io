{
  "private": true,
  "name": "adriancmiranda",
  "version": "1.0.0",
  "description": "My new git-push powered folio",
  "author": "Adrian C. Miranda",
  "license": "GNU AGPLv3",
  "main": "server/index.js",
  "browser": "client/index.js",
  "module": "index",
  "scripts": {
    "postinstall": "bower install",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && yarn",
    "test": "echo \"Error: no test specified\" && exit 0",
    "travis:test": "npm run test",
    "appveyor:test": "npm run test",
    "start": "node --optimize_for_size --max_old_space_size=920 --gc_interval=100 --use_strict --harmony server",
    "build": "webpack --config @bin --env run=client-build,server-build",
    "stats": "webpack --config @bin --profile --json > stats.json",
    "dev": "webpack --config @bin --env run=client-server,server-watch --env.dev"
  },
  "optionalDependencies": {
    "compression-webpack-plugin": "0.3.2",
    "webpack-bundle-analyzer": "2.3.1"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-istanbul": "4.1.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-preset-env": "1.3.3",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "clean-webpack-plugin": "0.1.16",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "4.0.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.4",
    "extract-loader": "0.1.0",
    "file-loader": "0.11.0",
    "friendly-errors-webpack-plugin": "1.6.1",
    "git-revision-webpack-plugin": "2.4.1",
    "html-webpack-plugin": "2.28.0",
    "imports-loader": "0.7.1",
    "json-loader": "0.5.4",
    "node-sass": "4.5.2",
    "npm-check-updates": "2.10.5",
    "npm-run-all": "4.0.2",
    "optimize-css-assets-webpack-plugin": "1.3.0",
    "pug-loader": "2.3.0",
    "sass-loader": "6.0.3",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-cfg": "1.2.3",
    "webpack-node-externals": "1.5.4"
  },
  "dependencies": {
    "express": "4.15.2",
    "is": "3.2.1",
    "moment": "2.18.1",
    "pirate-flag": "0.0.1"
  },
  "engines": {
    "node": ">=7.8.0",
  },
  "browsers": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
